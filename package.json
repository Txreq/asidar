    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "sass": "^1.55.0",
    "cross-env": "^7.0.3",
    "electromon": "^1.0.10",
    "electron": "^21.1.0",
    "electron-builder": "^23.6.0",
    "electron-squirrel-startup": "^1.0.0",
