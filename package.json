{
  "name": "asidar",
  "version": "0.7.1",
  "author": "Tareq Dev",
  "description": "a software which converts youtube video to mp3 files",
  "private": true,
  "main": "main.js",
  "homepage": "./",
  "dependencies": {
    "bytenode": "^1.3.6",
    "electron-store": "^8.1.0",
    "ffmetadata": "^1.7.0",
    "ffmpeg-static": "^5.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "ytdl-core": "^4.11.2",
    "ytdl-getinfo": "^1.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "webpack:compile": "webpack",
    "electron:start": "wait-on tcp:3000 && cross-env MODE=\"dev\" electron main.js",
    "electron:serve": "npm run webpack:compile && concurrently \"npm run start\" \"npm run electron:start\"",
    "electron:prod": "electron .",
    "electron:build": "npm run build && node builder.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@emotion/react": "^11.10.4",
    "@mantine/core": "^5.5.5",
    "@mantine/hooks": "^5.5.5",
    "@tabler/icons": "^1.103.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.64",
    "@types/react": "^18.0.21",
